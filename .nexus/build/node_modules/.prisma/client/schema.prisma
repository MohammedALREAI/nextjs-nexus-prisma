// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Faq {
  id        Int      @default(autoincrement()) @id
  title     String
  createdAt DateTime @default(now())
  updatedAt DateTime
  body      String
}
